package com.zookeeper.mammal;

public class Gorilla extends Mammal {
	
	public void throwSomething(int objects) {
		energyLevel -= 5 * objects;
		System.out.println("Gorilla is happy");
//		System.out.println(Gorilla.energyLevel);

	}
	public void eatBananas(int amount) {
		energyLevel +=10*amount;
		System.out.println("Gorilla climbed a tree");
//		System.out.println(Gorilla.displayEnergy());

	}
	public void climb(int times) {
		energyLevel -= 10 * times;
		System.out.println("Gorilla throws at you");
//		System.out.println(Gorilla.displayEnergy());

	}
}
